{
    "manifest_version": 2,
    "name": "XENIA chrome extension",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon/icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "contentscript.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ]
}